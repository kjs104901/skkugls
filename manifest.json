{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "SKKU GLS",
  "version": "1.4",
  "description": "SKKU GLS on Chrome",
  "permissions": ["activeTab", "tabs", "declarativeContent", "storage", "*://*/*", "contentSettings", "downloads"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/gls16.png",
      "32": "images/gls32.png",
      "48": "images/gls48.png",
      "128": "images/gls128.png"
    }
  },
  "icons": {
    "16": "images/gls16.png",
    "32": "images/gls32.png",
    "48": "images/gls48.png",
    "128": "images/gls128.png"
  },
  "content_security_policy": "script-src 'self' http://127.0.0.1:7895; object-src 'self'",
  "manifest_version": 2
}